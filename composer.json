{
  "name": "buexplain/alarm",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "alarm",
    "weChat",
    "dingTalk",
    "feiShu"
  ],
  "description": "这个是一个基于hyperf(swoole驱动)的告警组件",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.1",
    "psr/log": "*",
    "monolog/monolog": "^3.0",
    "hyperf/contract": "^3.0",
    "hyperf/utils": "^3.0",
    "hyperf/guzzle": "^3.0",
    "hyperf/exception-handler": "^3.0",
    "hyperf/process": "^3.0",
    "hyperf/support": "^3.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Alarm\\": "src/"
    }
  },
  "require-dev": {
    "swoole/ide-helper": "@dev",
    "friendsofphp/php-cs-fixer": "^3.0",
    "hyperf/signal": "@dev"
  },
  "extra": {
    "hyperf": {
      "config": "Alarm\\ConfigProvider"
    }
  }
}
