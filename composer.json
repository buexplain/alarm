{
  "name": "buexplain/alarm",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "alarm",
    "weChat",
    "dingTalk"
  ],
  "description": "这个是一个基于hyperf的告警组件",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.2",
    "psr/log": "*",
    "monolog/monolog": "*",
    "hyperf/contract": "*",
    "hyperf/utils": "*",
    "hyperf/guzzle": "2.1.*",
    "hyperf/exception-handler": "*",
    "hyperf/process": "2.1.*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Alarm\\": "src/"
    }
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "^4.5",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "extra": {
    "hyperf": {
      "config": "Alarm\\ConfigProvider"
    }
  }
}