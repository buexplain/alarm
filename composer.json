{
  "name": "buexplain/alarm",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "alarm",
    "weChat",
    "dingTalk"
  ],
  "description": "这个是一个基于hyperf的告警组件",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "psr/log": "*",
    "monolog/monolog": "*",
    "hyperf/contract": "~2.2",
    "hyperf/utils": "~2.2",
    "hyperf/guzzle": "~2.2",
    "hyperf/exception-handler": "~2.2",
    "hyperf/process": "~2.2"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Alarm\\": "src/"
    }
  },
  "require-dev": {
    "swoole/ide-helper": "@dev",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "extra": {
    "hyperf": {
      "config": "Alarm\\ConfigProvider"
    }
  }
}
